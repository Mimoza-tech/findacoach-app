{"ast":null,"code":"import RequestItem from '../../components/requests/RequestItem.vue';\nexport default {\n  components: {\n    RequestItem\n  },\n  data() {\n    return {\n      isLoading: false,\n      error: null\n    };\n  },\n  computed: {\n    receivedRequests() {\n      return this.$store.getters['requests/requests'];\n    },\n    hasRequests() {\n      return this.$store.getters['requests/hasRequests'];\n    }\n  },\n  created() {\n    this.loadRequests();\n  },\n  methods: {\n    async loadRequests() {\n      this.isLoading = true;\n      try {\n        await this.$store.dispatch('requests/fetchRequests');\n      } catch (error) {\n        this.error = error.message || 'Something failed!';\n      }\n      this.isLoading = false;\n    },\n    handleError() {\n      this.error = null;\n    }\n  }\n};","map":{"version":3,"mappings":"AA0BA,OAAOA,WAAU,MAAO,2CAA2C;AAEnE,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgB,GAAG;MACjB,OAAO,IAAI,CAACC,MAAM,CAACC,OAAO,CAAC,mBAAmB,CAAC;IACjD,CAAC;IACDC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACF,MAAM,CAACC,OAAO,CAAC,sBAAsB,CAAC;IACpD;EACF,CAAC;EACDE,OAAO,GAAG;IACR,IAAI,CAACC,YAAY,EAAE;EACrB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,YAAY,GAAG;MACnB,IAAI,CAACR,SAAQ,GAAI,IAAI;MACrB,IAAI;QACF,MAAM,IAAI,CAACI,MAAM,CAACM,QAAQ,CAAC,wBAAwB,CAAC;MACtD,EAAE,OAAOT,KAAK,EAAE;QACd,IAAI,CAACA,KAAI,GAAIA,KAAK,CAACU,OAAM,IAAK,mBAAmB;MACnD;MACA,IAAI,CAACX,SAAQ,GAAI,KAAK;IACxB,CAAC;IACDY,WAAW,GAAG;MACZ,IAAI,CAACX,KAAI,GAAI,IAAI;IACnB;EACF;AACF,CAAC","names":["RequestItem","components","data","isLoading","error","computed","receivedRequests","$store","getters","hasRequests","created","loadRequests","methods","dispatch","message","handleError"],"sourceRoot":"","sources":["/Users/mimozaneziri/Downloads/findacoach-app 2/src/pages/requests/RequestsReceived.vue"],"sourcesContent":["<template>\n  <div>\n    <base-dialog :show=\"!!error\" title=\"An error occurred!\" @close=\"handleError\">\n      <p>{{ error }}</p>\n    </base-dialog>\n    <section>\n      <base-card>\n        <header>\n          <h2>Requests Received</h2>\n        </header>\n        <base-spinner v-if=\"isLoading\"></base-spinner>\n        <ul v-else-if=\"hasRequests && !isLoading\">\n          <request-item\n            v-for=\"req in receivedRequests\"\n            :key=\"req.id\"\n            :email=\"req.userEmail\"\n            :message=\"req.message\"\n          ></request-item>\n        </ul>\n        <h3 v-else>You haven't received any requests yet!</h3>\n      </base-card>\n    </section>\n  </div>\n</template>\n\n<script>\nimport RequestItem from '../../components/requests/RequestItem.vue';\n\nexport default {\n  components: {\n    RequestItem,\n  },\n  data() {\n    return {\n      isLoading: false,\n      error: null,\n    };\n  },\n  computed: {\n    receivedRequests() {\n      return this.$store.getters['requests/requests'];\n    },\n    hasRequests() {\n      return this.$store.getters['requests/hasRequests'];\n    },\n  },\n  created() {\n    this.loadRequests();\n  },\n  methods: {\n    async loadRequests() {\n      this.isLoading = true;\n      try {\n        await this.$store.dispatch('requests/fetchRequests');\n      } catch (error) {\n        this.error = error.message || 'Something failed!';\n      }\n      this.isLoading = false;\n    },\n    handleError() {\n      this.error = null;\n    },\n  },\n};\n</script>\n\n<style scoped>\nheader {\n  text-align: center;\n}\n\nul {\n  list-style: none;\n  margin: 2rem auto;\n  padding: 0;\n  max-width: 30rem;\n}\n\nh3 {\n  text-align: center;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}